{
	"extends": "eslint:recommended",
	"rules": {
		// Possible problems
		"no-await-in-loop": [
			"warn"
		],
		"no-constant-binary-expression": [
			"warn"
		],
		"no-constructor-return": [
			"error"
		],
		"no-duplicate-imports": [
			"warn"
		],
		"no-new-native-nonconstructor": [
			"error"
		],
		"no-promise-executor-return": [
			"warn"
		],
		"no-self-compare": [
			"warn"
		],
		"no-unmodified-loop-condition": [
			"error"
		],
		"no-unreachable-loop": [
			"warn"
		],
		"no-unsafe-optional-chaining": [
			"warn",
			{
				"disallowArithmeticOperators": false
			}
		],
		"no-unused-private-class-members": [
			"warn"
		],
		"no-unused-vars": [
			"warn",
			{
				"args": "all",
				"argsIgnorePattern": "^_",
				"vars": "local",
				"varsIgnorePattern": "^_",
				"caughtErrors": "all",
				"destructuredArrayIgnorePattern": "^_",
				"ignoreRestSiblings": false
			}
		],
		"no-use-before-define": [
			"error",
			{
				"functions": true,
				"classes": true,
				"variables": true,
				"allowNamedExports": false
			}
		],
		"require-atomic-updates": [
			"warn",
			{
				"allowProperties": true
			}
		],
		// Suggestions
		"accessor-pairs": [
			"warn",
			{
				"getWithoutSet": true,
				"setWithoutGet": true,
				"enforceForClassMembers": true
			}
		],
		"arrow-body-style": [
			"warn",
			"as-needed"
		],
		"block-scoped-var": [
			"warn"
		],
		"camelcase": [
			"warn",
			{
				"allow": [],
				"properties": "never",
				"ignoreDestructuring": false,
				"ignoreGlobals": true,
				"ignoreImports": true
			}
		],
		"capitalized-comments": [
			"warn",
			"always",
			{
				"line": {},
				"block": {}
			}
		],
		"class-methods-use-this": [
			"off",
			{
				"enforceForClassFields": false,
				"exceptMethods": []
			}
		],
		"complexity": [
			"warn",
			{
				"max": 5
			}
		],
		"consistent-return": [
			"error",
			{
				"treatUndefinedAsUnspecified": true
			}
		],
		"consistent-this": [
			"off"
		],
		"curly": [
			"warn",
			"multi",
			"consistent"
		],
		"default-case": [
			"error"
		],
		"default-case-last": [
			"error"
		],
		"default-param-last": [
			"error"
		],
		"dot-notation": [
			"off"
		],
		"eqeqeq": [
			"error",
			"smart"
		],
		"func-name-matching": [
			"off"
		],
		"func-style": [
			"off"
		],
		"grouped-accessor-pairs": [
			"warn",
			"getBeforeSet"
		],
		"guard-for-in": [
			"warn"
		],
		"id-denylist": [
			"off"
		],
		"id-length": [
			"off"
		],
		"id-match": [
			"off"
		],
		"init-declarations": [
			"error",
			"always"
		],
		"logical-assignment-operators": [
			"off",
			"never"
		],
		"max-classes-per-file": [
			"warn",
			{
				"max": 2,
				"ignoreExpressions": true
			}
		],
		"max-depth": [
			"warn",
			{
				"max": 4
			}
		],
		"max-lines": [
			"warn",
			{
				"max": 500,
				"skipBlankLines": false,
				"skipComments": true
			}
		],
		"max-lines-per-function": [
			"off",
			{
				"IIFEs": true,
				"max": 30,
				"skipBlankLines": true,
				"skipComments": true
			}
		],
		"max-nested-callbacks": [
			"warn",
			{
				"max": 3
			}
		],
		"max-params": [
			"warn",
			{
				"max": 3
			}
		],
		"max-statements": [
			"off",
			{
				"max": 15
			}
		],
		"multiline-comment-style": [
			"warn",
			"starred-block"
		],
		"new-cap": [
			"error",
			{
				"newIsCap": true,
				"capIsNew": true,
				"newIsCapExceptions": [],
				"newIsCapExceptionPattern": "",
				"capIsNewExceptionPattern": "",
				"capIsNewExceptions": [],
				"properties": true
			}
		],
		"no-alert": [
			"error"
		],
		"no-array-constructor": [
			"off"
		],
		"no-bitwise": [
			"off",
			{
				"allow": [],
				"int32Hint": true
			}
		],
		"no-caller": [
			"error"
		],
		"no-case-declarations": [
			"off"
		],
		"no-confusing-arrow": [
			"error",
			{
				"allowParens": true,
				"onlyOneSimpleParam": false
			}
		],
		"no-console": [
			"off"
		],
		"no-continue": [
			"off"
		],
		"no-delete-var": [
			"error"
		],
		"no-div-regex": [
			"warn"
		],
		"no-else-return": [
			"warn",
			{
				"allowElseIf": false
			}
		],
		"no-empty": [
			"warn",
			{
				"allowEmptyCatch": false
			}
		],
		"no-empty-function": [
			""
		]
		// Layout & Formatting
	}
}